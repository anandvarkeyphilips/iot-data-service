<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>azure-service-principal-auth</id>
            <configuration>
                <client>${env.VARKEYS_EV_AZURE_SERVICE_PRINCIPAL_USERNAME}</client>
                <key>${env.VARKEYS_EV_AZURE_SERVICE_PRINCIPAL_PASSWORD}</key>
                <tenant>${env.VARKEYS_EV_AZURE_SERVICE_PRINCIPAL_TENANT}</tenant>
                <environment>AZURE</environment>
            </configuration>
        </server>
    </servers>
</settings>